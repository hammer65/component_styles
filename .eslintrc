{
  "ecmaFeatures": {
    "classes": true,
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "jest": true,
    "jquery": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-uses-react": 1
  },
  "globals": {
    "jQuery": true,
    "$": true
  }
}
